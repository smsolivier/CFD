FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (upper lower);

upper
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1.004e-5;
    rho             [1 -3 0 0 0 0 0] 998;
    // rho             [1 -3 0 0 0 0 0] 2000;
}

lower
{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] 1.004e-5;
    rho             [1 -3 0 0 0 0 0] 998;
}

sigma           [1 0 -2 0 0 0 0] 0.07;


