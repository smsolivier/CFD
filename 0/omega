FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;
// internalField      uniform 9.9; 

boundaryField
{
    walls
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    topInlet
    {
        // type            zeroGradient;
        type fixedValue; 
        // value uniform 1.49e-2; 
        value $internalField; 
    }

    bottomInlet
    {
        // type zeroGradient; 
        type fixedValue; 
        // value uniform 1.49e-2; 
        value $internalField; 
    }
    outlet
    {
        type            zeroGradient;
    }

    reflecting 
    {
        type zeroGradient; 
    }
    frontBack 
    {
        type            empty;
    }
}


// ************************************************************************* //