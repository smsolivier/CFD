FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    topInlet
    {
        type            fixedValue;
        value           uniform (0.6 0 0); 
    }

    bottomInlet
    {
        type            fixedValue;
        value           uniform (0.6 0 0); 
    }

    outlet
    {
        type zeroGradient; 
    }

    walls
    {
        type noSlip; 
    }

    reflecting
    {
        type zeroGradient; 
    }

    frontBack
    {
        type empty; 
    }
}

// ************************************************************************* //
